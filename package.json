{
    "name": "zoho-attendance-manager",
    "version": "1.0.0",
    "description": "Automated Zoho attendance check-in/out with cron scheduling",
    "main": "attendance_manager.js",
    "scripts": {
        "start": "node attendance_manager.js",
        "test-checkin": "node -e \"require('./attendance_manager').checkIn().catch(console.error)\"",
        "test-checkout": "node -e \"require('./attendance_manager').checkOut().catch(console.error)\""
    },
    "dependencies": {
        "axios": "^1.6.0",
        "node-cron": "^3.0.3",
        "dotenv": "^16.3.1"
    },
    "keywords": [
        "zoho",
        "attendance",
        "automation",
        "cron"
    ],
